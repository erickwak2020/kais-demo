<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="gov.mopas.kais.board.dao.BoardMapper">
    <select id="findAll" resultType="gov.mopas.kais.board.model.Board">
        select * from BOARD;
    </select>

    <select id="findById" resultType="gov.mopas.kais.board.model.Board">
        select * from BOARD where ID = #{id}
    </select>

    <!--<insert id="save" useGeneratedKeys="true" keyProperty="id">
        insert into MEMBER (USERNAME, NICKNAME, PASSWORD)
        values (#{username}, #{password}, #{nickname});
    </insert>-->
</mapper>